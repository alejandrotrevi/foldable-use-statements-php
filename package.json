{
  "name": "foldable-use-statements",
  "displayName": "Foldable use statements",
  "description": "Simple extension to add the option to fold use statements in PHP classes",
  "version": "0.0.3",
  "publisher": "alex-tf",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:php"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "php",
        "extensions": [".php"],
        "aliases": ["PHP", "php"]
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
